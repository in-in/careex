mixin social()
  mj-social(
    base-url=pathToIconSocial
    display='twitter facebook dribbble'
    dribbble-content='Dribbble'
    dribbble-href='https://dribbble.com/'
    dribbble-icon-color='#fff'
    dribbble-icon=pathToIconSocial + 'dribbble.png'
    facebook-icon-color='#fff'
    icon-size='16px'
    inner-padding='5px'
    line-height='16px'
    padding='0'
    text-mode='false'
    twitter-icon-color='#fff')/
